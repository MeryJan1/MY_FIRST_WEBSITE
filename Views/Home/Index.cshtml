    <!-- Start About -->
    <section id="about-cont" class="about">
        <div class="title">
            <h1 class="kaushan">HAKKIMDA</h1>
        </div>
        <div class="container">

            <div class="row">
                <div class="col-md-10 col-lg-6">
                    <div class="about-img bg-image"></div>
                    <!-- go to main.scss file to edit personal image -->
                </div>
                <div class="col-md-12 col-lg-6">
                    <div class="tabs-about">
                        <ul class="nav nav-tabs text-center" id="myTab" role="tablist">

                            <!-- Start About Me Title Tab -->
                            <li class="nav-item">
                                <a class="nav-link active" id="about-tab" data-toggle="tab" href="#aboutme" role="tab" aria-controls="about" aria-expanded="true">
                                    <i class="icon-male-user"></i>
                                    <span>Hakkımda</span>
                                </a>
                            </li>
                            <!-- End About Me Title Tab -->
                            <!-- ================ -->
                            <!-- Start Profile Title Tab -->
                            <li class="nav-item">
                                <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile">
                                    <i class="icon-star"></i>
                                    <span>Profil</span>
                                </a>
                            </li>
                            <!-- End Profile Title Tab -->
                            <!-- ================ -->
                            <!-- Start Skills Title Tab -->
                            <li class="nav-item">
                                <a class="nav-link" id="skill-tab" data-toggle="tab" href="#skill" role="tab" aria-controls="skill">
                                    <i class="icon-magic-wand"></i>
                                    <span>Yetenekler</span>
                                </a>
                            </li>
                            <!-- End Skills Title Tab -->
                        </ul>
                        <div class="tab-content" id="myTabContent">

                            <!-- Start About Me Tab -->
                            <div class="tab-pane fade show active text-left" id="aboutme" role="tabpanel" aria-labelledby="about-tab">
                                <p> @ViewBag.About.AboutMe</p>
                                <img src="/assets/images/signature.png" class="img-fluid signature" alt="signature" />
                                <!-- Here image Signature -->
                            </div>
                            <!-- End About Me Tab -->
                            <!-- ================ -->
                            <!-- Start Profile Tab -->
                            <div class="tab-pane fade text-left profile" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                                <h6><span class="title-info">İsim :</span><span>@ViewBag.Profil.Name</span></h6> <!-- Edit Your Name  -->
                                <h6><span class="title-info">Uyruğu : </span><span>@ViewBag.Profil.Nationality</span></h6> <!-- Edit Your Nationality  -->
                                <h6><span class="title-info">Email : </span><span>@ViewBag.Profil.Email</span></h6> <!-- Edit Your Email  -->
                                <h6><span class="title-info">Doğum Tarihi : </span><span>@ViewBag.Profil.DateOfBirth</span></h6> <!-- Edit Your Date Of Birth  -->
                                <h6><span class="title-info">Telefon NUmarası : </span><span>0 @ViewBag.Profil.Phone</span></h6> <!-- Edit Your Phone -->

                                <div class="btn-about">
                                    <a href="#">Özgeçmişi İndir</a>
                                </div>
                            </div>
                            <!-- End Profile Tab -->
                            <!-- ================ -->
                            <!-- Start Skills Tab -->
                            <div class="tab-pane fade skill" id="skill" role="tabpanel" aria-labelledby="skill-tab">
                                @foreach (var item in ViewBag.Skills)
                                {
                                    <div class="skills">
                                        <p class="progress-text">@item.Skill</p> <!-- Here You Can Edit The Name Of Skill -->
                                        <div class="progress">
                                            <div class="progress-bar" data-present="@item.Percentage%"></div> <!-- Here You Can Edit The Percentage -->
                                        </div>

                                    </div>
                                }

                                <div class="btn-about">
                                    <a href="#contact-cont">İşe Al</a>
                                </div>
                            </div>
                            <!-- End Skills Tab -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

