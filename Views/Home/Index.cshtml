                <a href="@ViewBag.Navbar.Linkedin" target="_blank"><i class="fab fa-linkedin"></i></a> <!-- You Can Edit the link here -->
                <a href="@ViewBag.Navbar.Youtube" target="_blank"><i class="fab fa-youtube"></i></a> <!-- You Can Edit the link here -->
