@model IEnumerable<ProjeSon.Models.Services.ServicesInfo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Icon1URL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.String11)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.String12)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Icon1URL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.String11)
